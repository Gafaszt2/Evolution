{
		"name": "Thrinaxodon",
		"unitType": "Land Chordata",
		"movement": 2,
		"strength": 100,
		"cost": 120,
                "requiredTech": "First Mammals",
                "uniques": ["[+60]% to Flank Attack bonuses"],
